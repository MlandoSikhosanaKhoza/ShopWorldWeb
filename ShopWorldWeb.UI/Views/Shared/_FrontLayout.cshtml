@{
    Layout = "~/Views/Shared/_DefaultLayout.cshtml";
}
@section CSS{
    <meta content="" name="description">
    <meta content="" name="keywords">

    <!-- Favicons -->
    <link href="~/image/front/favicon.png" rel="icon">
    <link href="~/image/front/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Raleway:300,300i,400,400i,600,600i,700,700i" rel="stylesheet">

    <!-- Vendor CSS Files -->
    <link href="~/css/front/bootstrap/bootstrap.min.css" rel="stylesheet" />
    <link href="~/css/front/bootstrap-icons.css" rel="stylesheet" />
    <link href="~/css/front/style.css" rel="stylesheet" />
    @RenderSection("css", required: false)
}
@RenderBody()
@section Scripts{
    <script src="~/js/front/main.js"></script>
    @RenderSection("scripts", required: false)
}